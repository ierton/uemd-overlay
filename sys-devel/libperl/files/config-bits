crypt_r_proto='$(use elibc_uclibc && echo '0' || echo 'REENTRANT_PROTO_B_CCS')'
cryptlib=''
csh='csh'
ctermid_r_proto='0'
ctime_r_proto='REENTRANT_PROTO_B_SB'
d_Gconvert='gcvt((x),(n),(b))'
d_PRIEUldbl='define'
